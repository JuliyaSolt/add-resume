{{#with service}}
    <div class="card text-bg-success">
        <div class="card-body text-center">   
        <h2 class="card-title text-center">{{title}}</h2>
        <p>{{description}}</p>
        </div>
        <div class="text-center pb-3">
        {{#each buttons}}
            <a href={{link}} class="btn text-bg-primary">{{text}}</a>
        </div>
        {{/each}}     
    </div> 
{{/with}}  