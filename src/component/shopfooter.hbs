{{#each footer}}
    <div class="nav col-2">
        <div class="m-3">
            {{#each this}}
            <div>
                <a class="nav-link {{#if @last}} disabled {{/if}}"
                 href={{this.link}}>{{this.text}}</a>  
            </div>
             {{/each}}
         </div>
    </div>
{{/each}}